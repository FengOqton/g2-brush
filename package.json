{
  "name": "@antv/g2-plugin-brush",
  "version": "0.0.1",
  "description": "",
  "main": "build/g2-plugin-brush.js",
  "browser": "build/g2-plugin-brush.min.js",
  "module": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:antvis/g2-plugin-brush.git"
  },
  "keywords": [
    "antv",
    "g2-plugin-brush",
    "g2PluginBrush"
  ],
  "author": "https://github.com/orgs/antvis/people",
  "license": "MIT",
  "devDependencies": {
    "@lite-js/torch": "^0.2.6",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.1",
    "commander": "^2.9.0",
    "connect": "^3.6.3",
    "d3-queue": "^3.0.7",
    "debug": "^3.1.0",
    "electron": "~1.6.11",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-egg": "^4.2.0",
    "eslint-plugin-html": "^3.1.1",
    "get-port": "^3.1.0",
    "nightmare": "^2.10.0",
    "nunjucks": "^3.0.1",
    "open": "0.0.5",
    "parseurl": "^1.3.1",
    "pre-commit": "^1.2.2",
    "serve-static": "^1.12.4",
    "shelljs": "^0.7.8",
    "uglify-js": "^3.0.15",
    "webpack": "^3.3.0"
  },
  "scripts": {
    "build": "webpack",
    "ci": "npm run lint && npm run test",
    "compress": "uglifyjs --compress --mangle --output dist/g2-plugin-brush.min.js -- build/g2-plugin-brush.js",
    "coverage": "npm run coverage-generator && npm run coverage-viewer",
    "coverage-generator": "torch --coverage --renderer --recursive test/unit",
    "coverage-viewer": "torch-coverage",
    "demos": "$(npm bin)/electron ./demos/app.js",
    "demos-web": "node ./demos/app.js --web --port 2046",
    "dev": "npm run watch & DEBUG=app:* npm run demos-web",
    "dist": "rm -rf dist && mkdir dist && npm run build && npm run compress",
    "lint": "eslint --ext .html,.js ./",
    "lint-fix": "eslint --ext .html,.js --fix ./",
    "prepublish": "npm run dist",
    "screenshot": "DEBUG=app:* ./bin/screenshot.js",
    "test": "torch --renderer --recursive ./test/unit",
    "test-live": "torch --interactive --watch --recursive ./test/unit",
    "watch": "webpack --config webpack-dev.config.js"
  },
  "pre-commit": {
    "run": [
      "lint",
      "build",
      "test"
    ],
    "silent": false
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
